{
  "name": "address-book",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build && npm run rm-client-source-maps",
    "dev": "cross-env NODE_ENV=development node server.js",
    "start": "node server.js",
    "typecheck": "react-router typegen && tsc",
    "rm-client-source-maps": "find build/client -type f -name '*.map' -delete"
  },
  "dependencies": {
    "@react-router/node": "^7.1.4",
    "@react-router/serve": "^7.1.4",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "isbot": "^5.1.17",
    "match-sorter": "^8.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.4",
    "sort-by": "^1.2.0",
    "source-map": "^0.7.4",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.4",
    "@types/express": "^5.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "cross-env": "^7.0.3",
    "prettier": "3.4.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
